{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "baseUrl": ".",
    "paths": {
      "@hooks/*": ["./hooks/*"],
      "@components/*": ["./components/*"],
      "@stores/*": ["./stores/*"],
      "@utils/*": ["./utils/*"]
    },
    "types": ["nativewind/types"]
  },
  "include": ["./**/*"]
}
